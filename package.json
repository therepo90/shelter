{
  "name": "shelter",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "https://ionicframework.com/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "git-info": "git rev-parse HEAD | head -c 8 > .git-version && git add .git-version && git commit -m \"update git version\" --allow-empty && git push -u",
    "postbuild": "mkdir -p releases && cd www && zip -r ../releases/www.zip . && cd .. && cp /releases/www.zip src/www.zip",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "deploy:pages": "ng deploy --base-href=/shelter/",
    "ionserve": "ionic serve",
    "ion:build:web": "ionic build",
    "mobiledep": "ionic build && ionic cap copy && ionic cap sync && ionic cap open android",
    "deploy": "npm run mobiledep && git add . && git commit -m \"rls\" --allow-empty && git push -u && ng deploy --base-href=/ && npm run git-info"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.0.0",
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/platform-browser": "^20.0.0",
    "@angular/platform-browser-dynamic": "^20.0.0",
    "@angular/router": "^20.0.0",
    "@capacitor/android": "8.0.0",
    "@capacitor/app": "8.0.0",
    "@capacitor/core": "8.0.0",
    "@capacitor/haptics": "8.0.0",
    "@capacitor/keyboard": "8.0.0",
    "@capacitor/preferences": "^8.0.0",
    "@capacitor/splash-screen": "^8.0.0",
    "@capacitor/status-bar": "8.0.0",
    "@capgo/capacitor-updater": "^8.40.8",
    "@ionic/angular": "^8.0.0",
    "@ionic/pwa-elements": "^3.3.0",
    "@ionic/storage-angular": "^4.0.0",
    "ionicons": "^7.0.0",
    "js-sha256": "^0.11.1",
    "localforage": "^1.10.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
